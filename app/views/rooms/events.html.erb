<%- model_class = Room -%>
<% page_header = t '.title', :event => Event.model_name.human.pluralize.titleize, :room => @room.name %>
<% title page_header %>
<div class="page-header">
  <h1><%= page_header %></h1>
</div>

<%= render :partial => 'upcoming_events' %>

<%= link_to t('.back', :default => t("helpers.links.back")),
              room_path(@room), :class => 'btn btn-default'  %>